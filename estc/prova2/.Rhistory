r = function(x,y){
n = length(x)
somatorio1 = sum(x*y)
somatoriox2 = sum(x*x)
somatorioy2 = sum(y*y)
numer = somatorio1 - n*mean(x)*mean(y)
denom = sqrt(somatoriox2-n*mean(x)*mean(x)) * sqrt(somatorioy2-n*mean(y)*mean(y))
return (numer/denom)
}
